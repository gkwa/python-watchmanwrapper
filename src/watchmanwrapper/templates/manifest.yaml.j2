    dir: /root/celery_test
    user: juniorjim
    host: k3d.mydomain.com
  sync: /usr/local/bin/rsync --verbose --archive --compress --no-links --exclude=.venv --exclude=^#*# --exclude=__pycache__ --exclude=*.log --exclude=.* SRC/ DST
